class groundwall{
    constructor(x,y,width,height)
    {
        var options={
            isStatic:true
        }
        this.body=Bodies.rectangle(400,690,50,20);
        this.width=width;
        this.height=height;
        World.add(world,this.body);
    }
    display()
    {
        rectMode(CENTER);
        fill(220);
        rect(this.body.position.x,this.body.position.y,this.width,this.height);
    }
}
